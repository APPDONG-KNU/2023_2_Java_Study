//백준 문자열 10번
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String str = sc.next();
		int strLen = str.length(), sum = 0;
		
		for(int i = 0; i < strLen; i++) {
			int num = str.charAt(i) - 'A';
			if(num < 15) 
				sum += num / 3 + 3;
			else if(num <= 18)
				sum += 8;
			else if(num <= 21)
				sum += 9;
			else
				sum += 10;
		}
		System.out.print(sum);
		sc.close();
	}
}
